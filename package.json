{
  "name": "goon.vc",
  "version": "0.1.0-RC1",
  "description": "goon.vc public",
  "main": "services/goon.vc.git",
  "scripts": {
    "build": "npm run build:browser",
    "build:browser": "node scripts/build.js",
    "docs": "npm run make:api && npm run make:docs && http-server -p 8000 docs",
    "make:api": "jsdoc2md --configure jsdoc.json --files types/*.js schemata/*.js services/*.js settings/deprecations.js README.md > API.md",
    "report:install": "rm -rf node_modules && echo \"\n\" > package-lock.json && echo \"$ npm i\" > reports/install.log && npm i >> reports/install.log",
    "start": "node scripts/node.js",
    "test": "mocha tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoonCitizen/goon.vc.git"
  },
  "keywords": [
    "star",
    "citizen",
    "fabric",
    "bitcoin",
    "sensemaker"
  ],
  "author": "Fabric Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GoonCitizen/goon.vc/issues"
  },
  "homepage": "https://github.com/GoonCitizen/goon.vc#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@babel/register": "^7.26.0"
  },
  "dependencies": {
    "@fabric/star-citizen-live": "GoonCitizen/star-citizen-live#feature/discord"
  }
}
